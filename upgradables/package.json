{
  "name": "@gooddollar/goodcontracts-upgradables",
  "version": "2.2.0",
  "description": "GoodDollar Contracts",
  "scripts": {
    "setenv": "set -o allexport; source ../.env; set +o allexport",
    "build": "scripts/build.sh deploy",
    "start": "run-s wait start:withmain",
    "start:main": "cd ../stakingModel && npm start",
    "start:withmain": "run-s start:main deploy",
    "coverage:report": "export COVERALLS_REPO_TOKEN='qiXNelh1HXMAsoPah9yRD2yG2WlqrDfLh' && cat ../coverage/lcov.info coverage/lcov.info | coveralls",
    "coverage": "export ADMIN_MNEMONIC='myth like bonus scare over problem client lizard pioneer submit female collect' && export MNEMONIC='glad notable bullet donkey fall dolphin simple size stone evil slogan dinner' && truffle run coverage --network test",
    "ganache": "ganache-cli -m \"$MNEMONIC\" -b 1 -g 1000000 -l 9000000 --port 9545 --networkId 4447 -q",
    "ganache:test": "ganache-cli -m \"$MNEMONIC\" -g 1000000 -l 9000000 --port 9545 --networkId 4447 -q",
    "wait": "wait-on tcp:9545",
    "minimize": "../scripts/minimize.sh 'build/contracts'",
    "deploy": "run-s migrate:full",
    "test:tdd": "export NETWORK='test' && export ADMIN_MNEMONIC='myth like bonus scare over problem client lizard pioneer submit female collect' && export MNEMONIC='glad notable bullet donkey fall dolphin simple size stone evil slogan dinner' && run-p ganache:test test:wait:tdd",
    "test:wait:tdd": "run-s wait && truffle test --network tdd && pkill -SIGTERM -f ganache",
    "test:full": "export NETWORK='test' && export ADMIN_MNEMONIC='myth like bonus scare over problem client lizard pioneer submit female collect' && export MNEMONIC='glad notable bullet donkey fall dolphin simple size stone evil slogan dinner' && run-p ganache:test test:wait:full",
    "test:wait:full": "run-p wait start:main && truffle test && pkill -SIGTERM -f ganache",
    "test": "npm run test:full",
    "lint": "solhint --formatter codeframe develop/**/*.sol",
    "migrate:full": "truffle migrate --network ${NETWORK-develop} --reset",
    "simulate:interest": "truffle exec scripts/simulateInterestDays.js --network develop"
  },
  "files": [
    "build",
    "contracts",
    "migrations",
    "releases",
    "scripts",
    "truffle-config.js",
    "package-lock.json",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GoodDollar/GoodContracts.git"
  },
  "keywords": [
    "UBI",
    "basic",
    "income",
    "gooddollar",
    "ethereum",
    "smart",
    "contracts"
  ],
  "author": "GoodDollar Community <builders@gooddollar.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GoodDollar/GoodContracts/issues"
  },
  "homepage": "https://gooddollar.org",
  "dependencies": {
    "ethers-eip712": "^0.2.0"
  },
