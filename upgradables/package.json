    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "bignumber.js": "^8.1.1",
    "bindings": "^1.4.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "dotenv": "^6.2.0",
    "eth-gas-reporter": "^0.2.4",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.0.24",
    "fkill-cli": "^5.2.0",
    "fs-extra": "^7.0.1",
